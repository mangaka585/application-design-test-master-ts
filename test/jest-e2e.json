{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "maxWorkers": 1,
  "globalSetup": "<rootDir>/setup/e2e/global.setup.ts",
  "setupFilesAfterEnv": ["<rootDir>/setup/e2e/setupFile.ts"]
}
